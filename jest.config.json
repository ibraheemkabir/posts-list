{
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
        "src/**/*.{js,jsx,ts,tsx}",
        "!<rootDir>/node_modules/",
        "!<rootDir>/src/main.tsx",
        "!src/types/*.{js,jsx,ts,tsx}"
    ],
    "testPathIgnorePatterns": [
        "fixtures"
    ],
    "moduleNameMapper": {
        "\\.(scss|css)$": "identity-obj-proxy",
        "^.+\\.svg$": "jest-svg-transformer"
    }
}